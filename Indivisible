#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,a,b,c,n=1;
	cin>>t;
	while(t>0)
	{
	    cin>>a>>b>>c;
	    while(n<100)
	    {
	        if((a%n!=0) && (b%n!=0) && (c%n!=0))
	        {
	            cout<<n<<endl;
	            break;
	        }
	        n++;
	    }
	    n=1;
	    t--;
	}
	return 0;
}
