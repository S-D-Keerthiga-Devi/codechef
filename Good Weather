#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,a[7],i,c1=0,c2=0,n=7;
	cin>>t;
	while(t>0)
	{
	    for(i=0;i<7;i++)
	    {
	        cin>>a[i];
	    }
	    for(i=0;i<7;i++)
	    {
	        if(a[i]==1)
	        {
	            c1++;
	        }
	        else
	        {
	            c2++;
	        }
	    }
	    if(c1>c2)
	    {
	        cout<<"YES"<<endl;
	    }
	    else
	    {
	        cout<<"NO"<<endl;
	    }
	    c1=0;
	    c2=0;
	    t--;
	}
	return 0;
}
