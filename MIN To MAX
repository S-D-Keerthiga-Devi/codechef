#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t>0)
	{
	    int n;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }
	    int count=0;
	    sort(a,a+n);
	    int c=a[0];
	    for(int i=0;i<n;i++)
	    {
	        if(a[i]>c)
	        {
	            count++;
	        }
	    }
	    cout<<count<<endl;
	    t--;
	}
	return 0;
}
