#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,a,b;
	cin>>t;
	while(t>0)
	{
	    cin>>a>>b;
	    if(a==b)
	    {
	        cout<<"Yes"<<endl;
	    }
	    else if((abs(a-b))%2==0)
	    {
	        cout<<"Yes"<<endl;
	    }
	    else
	    {
	        cout<<"No"<<endl;
	    }
	    t--;
	}
	return 0;
}
