#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x,y;
	cin>>t;
	while(t>0)
	{
	    cin>>x>>y;
	    if((y-x)%8==0)
	    {
	        cout<<(y-x)/8<<endl;
	    }
	    else if(x>=y)
	    {
	        cout<<"0"<<endl;
	    }
	    else
	    {
	        cout<<((y-x)/8)+1<<endl;
	    }
	    t--;
	}
	return 0;
}
