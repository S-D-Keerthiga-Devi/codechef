#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x1,y1,x2,y2;
	cin>>t;
	while(t>0)
	{
	    cin>>x1>>y1>>x2>>y2;
	    if((x1!=x2) && (y1!=y2))
	    {
	        cout<<"1"<<endl;
	    }
	    else if((x1==x2) || (y1==y2))
	    {
	        cout<<"2"<<endl;
	    }
	    else
	    {
	        cout<<"3"<<endl;
	    }
	    t--;
	}
	return 0;
}
