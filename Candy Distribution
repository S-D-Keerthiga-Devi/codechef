#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,m,x;
	cin>>t;
	while(t>0)
	{
	    cin>>n>>m;
	    if(n%m==0)
	    {
	        x=n/m;
	        if(x%2==0)
	        {
	            cout<<"Yes"<<endl;
	        }
	        else
	        {
	            cout<<"No"<<endl;
	        }
	    }
	    else
	    {
	        cout<<"No"<<endl;
	    }
	    t--;
	}
	return 0;
}
